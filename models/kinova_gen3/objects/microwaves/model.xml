<mujoco model="model">
  <compiler angle="radian"/>
  <asset>
    <texture file="visuals/material_0.png" name="tex-model" type="2d"/>
    <texture type="2d" name="image7" file="visuals/image7.png"/>
    <texture type="2d" name="image2" file="visuals/image2.png"/>
    <texture type="2d" name="image5" file="visuals/image5.png"/>
    <material name="model" reflectance="0.5" texrepeat="1 1" texture="tex-model" texuniform="false"/>
    <material name="TX_Microwave_3_1" texture="image7" specular="0.5" shininess="0.25"/>
    <material name="TX_Microwave_3_2" texture="image2" specular="0.5" shininess="0.25"/>
    <material name="TX_Microwave_3_3" texture="image5" specular="0.5" shininess="0.25"/>
    <mesh file="visuals/model_0.obj" name="model_0_vis" scale="0.5 0.5 0.5"/>
    <mesh file="visuals/model_1.obj" name="model_1_vis" scale="0.5 0.5 0.5"/>
    <mesh file="visuals/model_2.obj" name="model_2_vis" scale="0.5 0.5 0.5"/>
    <mesh file="visuals/door_0.obj" name="door_0_vis" scale="0.5 0.5 0.5"/>
    <mesh file="visuals/door_1.obj" name="door_1_vis" scale="0.5 0.5 0.5"/>
    <mesh file="visuals/door_2.obj" name="door_2_vis" scale="0.5 0.5 0.5"/>
  </asset>
  <default>
    <default class="visual">
      <geom conaffinity="0" contype="0" group="1" type="mesh"/>
    </default>
    <default class="collision">
      <geom group="0" rgba="0.5 0 0 0.5"/>
    </default>
  </default>
  <worldbody>
    <body>
      <body name="object" pos="1 0 0.15">
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_0_vis" material="TX_Microwave_3_2" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_1_vis" material="TX_Microwave_3_3" class="visual"/>
        <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="model_2_vis" material="TX_Microwave_3_1" class="visual"/>
        <!-- Numpad inside fix -->
        <geom class="visual" type="box" rgba="0.4 0.4 0.4 1" pos="0.18 -0.1845 0.0145" size="0.0475 0.0165 0.125"/>
        <geom class="visual" type="box" rgba="0.4 0.4 0.4 1" pos="0.18 -0.181 0.145" size="0.0475 0.015 0.0055"/>
        <geom class="visual" type="box" rgba="0.4 0.4 0.4 1" pos="0.18 -0.1815 -0.119" size="0.0475 0.015 0.0085"/>
        <!-- Back -->
        <geom class="collision" type="box" pos="0 0.21 0" size="0.2425 0.045 0.151"/>
        <!-- Bottom -->
        <geom class="collision" type="box" pos="0 0 -0.124" size="0.2425 0.167 0.0275"/>
        <!-- Left -->
        <geom class="collision" type="box" pos="-0.23 0 0" size="0.0125 0.167 0.151"/>
        <!-- Right -->
        <geom class="collision" type="box" pos="0.1675 0 0" size="0.074 0.167 0.151"/>
        <!-- Top -->
        <geom class="collision" type="box" pos="0 0 0.1365" size="0.2425 0.167 0.0145"/>
        <!-- Numpad -->
        <geom class="collision" type="box" pos="0.187 -0.1845 0.0115" size="0.0545 0.0185 0.1395"/>
        <!-- start/stop buttons -->
        <geom class="collision" type="box" pos="0.2125 -0.1925 -0.065" size="0.015 0.0108 0.0125" name="start_button" rgba="0.0 0.5 0 0.5"/>
        <geom class="collision" type="box" pos="0.15 -0.1925 -0.065" size="0.015 0.0108 0.0125" name="stop_button" rgba="0.0 0.5 0 0.5"/>
        <geom class="collision" type="cylinder" pos="-0.06 -0.01 -0.0975" size="0.11 0.0025" rgba="0 1 0 0.5" name="tray"/>
        <body name="door">
          <joint axis="0 0 1" limited="true" name="microjoint" range="-1.57 0" damping="2" frictionloss="2" armature=".01" pos="-0.2425 -0.167 0" ref="0"/>
          <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="door_0_vis" material="TX_Microwave_3_2" class="visual"/>
          <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="door_1_vis" material="TX_Microwave_3_3" class="visual"/>
          <geom solimp="0.998 0.998 0.001" solref="0.001 1" density="100" friction="0.95 0.3 0.1" type="mesh" mesh="door_2_vis" material="TX_Microwave_3_1" class="visual"/>
          <!-- Door Main -->
          <geom class="collision" type="box" pos="-0.045 -0.1855 0.0115" size="0.17595 0.0185 0.1395"/>
          <!-- Handle Top -->
          <geom class="collision" type="box" pos="0.102 -0.2265 0.118" size="0.0135 0.0235 0.0105"/>
          <!-- Handle Bottom -->
          <geom class="collision" type="box" pos="0.102 -0.2265 -0.0795" size="0.0135 0.0235 0.0105"/>
          <!-- Handle Main -->
          <geom class="collision" type="cylinder" pos="0.102 -0.237 0.0195" size="0.011 0.09 0" name="door_handle"/>
          <!-- door sites -->
          <site rgba="1 0 0 0" size="0.0025" pos="-0.0375 -0.20 0.005" name="door_p1"/>
          <site rgba="0 1 0 0" size="0.0025" pos="-0.0125 -0.20 0.005" name="door_p2"/>
          <site rgba="0 0 1 0" size="0.0025" pos="-0.0375 -0.20 0.03" name="door_p3"/>
        </body>
        <!-- exterior bounding box points -->
        <site rgba="0 0 0 1" size="0.005" pos="-0.2425 -0.204 -0.13" name="ext_p0"/>
        <site rgba="1 0 0 1" size="0.005" pos="0.2425 -0.204 -0.13" name="ext_px"/>
        <site rgba="0 1 0 1" size="0.005" pos="-0.2425 0.2325 -0.13" name="ext_py"/>
        <site rgba="0 0 1 1" size="0.005" pos="-0.2425 -0.204 0.1525" name="ext_pz"/>
        <!-- interior bounding box points -->
        <site rgba="0.5 0.5 0.5 1" size="0.005" pos="-0.2175 -0.167 -0.10" name="int_p0"/>
        <site rgba="0 1 1 1" size="0.005" pos="0.0935 -0.167 -0.10" name="int_px"/>
        <site rgba="1 1 0 1" size="0.005" pos="-0.2175 0.165 -0.10" name="int_py"/>
        <site rgba="1 0 1 1" size="0.005" pos="-0.2175 -0.167 0.122" name="int_pz"/>
      </body>
    </body>
  </worldbody>
</mujoco>
